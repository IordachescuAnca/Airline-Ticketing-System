                  <Button fx:id="signUpClient" layoutX="215.0" layoutY="340.0" mnemonicParsing="false" onAction="#signUpClient" text="Sign up!" />
                  <ImageView fx:id="backLogInClient" fitHeight="37.0" fitWidth="37.0" layoutX="14.0" layoutY="14.0" onMouseClicked="#backtoLogInClient" pickOnBounds="true" preserveRatio="true">
